{
  "name": "modreg",
  "description": "A Node.js module that provides registry handling of installable components.",
  "version": "0.1.3",
  "author": "Mihai Potra <mike@mpotra.com>",
  "license": "MIT",
  "repository": "mpotra/modreg",
  "keywords": [
    "registry",
    "register",
    "components",
    "modules",
    "plug-ins",
    "map"
  ],
  "main": "index.js",
  "dependencies": {
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jslint": "eslint src",
    "jslint-examples": "eslint examples",
    "compile": "babel -d lib/ src/lib",
    "compile-examples": "mkdir -p examples/compiled && babel -d examples/* examples/compiled",
    "prepublish": "npm run jslint && npm run compile"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "ignore": [
      "examples/compiled"
    ]
  }
}
